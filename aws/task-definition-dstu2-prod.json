{
  "executionRoleArn": "arn:aws:iam::657600230790:role/ecsTaskExecutionRole",
  "family": "api-dstu2-prod",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "api-dstu2-prod",
      "image": "TBD",
      "cpu": 1024,
      "memoryReservation": 1000,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/api-dstu2-prod",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 8075,
          "protocol": "tcp",
          "containerPort": 8075
        }
      ],
      "environment": [
      ],
      "essential": true,
      "mountPoints": [
        {
          "sourceVolume": "indexes",
          "containerPath": "/var/indexes"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [
    {
      "name": "indexes"
    }
  ]
}
