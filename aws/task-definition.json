{
  "family": "TBD",
  "containerDefinitions": [
    {
      "name": "TBD",
      "image": "TBD",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 0,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "memoryReservation": 0,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "TBD",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "mountPoints": [
        {
          "readOnly": false,
          "containerPath": "/var/indexes",
          "sourceVolume": "indexes"
        }
      ],
      "essential": true
    }
  ],
  "volumes": [
    {
      "name": "indexes",
      "host": {
        "sourcePath": "/var/ecs/indexes"
      }
    }
  ]
}
