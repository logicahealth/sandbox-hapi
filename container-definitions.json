[
  {
    "name": "{{CONTAINER_NAME}}-{{FHIR_VERSION}}",
    "image": "{{IMAGE_NAME}}",
    "cpu": 0,
    "portMappings": [
      {
        "containerPort": {{CONTAINER_PORT}},
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "memoryReservation": 900,
    "essential": true,
    "environment": [
      {
        "name": "SPRING_PROFILES_ACTIVE",
        "value": "{{FHIR_VERSION}},{{ENVIRONMENT}},multitenant"
      }
    ],
    "mountPoints": [
      {
        "readOnly": false,
        "containerPath": "/var/indexes",
        "sourceVolume": "indexes"
      }
    ]
  }
]
