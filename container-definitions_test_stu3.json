[
  {
    "name": "{{CONTAINER_NAME}}-stu3",
    "image": "nexus.hspconsortium.org:18083/hspc/{{CONTAINER_NAME}}:{{PROJECT_VERSION}}",
    "cpu": 0,
    "portMappings": [
      {
        "containerPort": 8076,
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "memoryReservation": 900,
    "essential": true,
    "environment": [
      {
        "name": "SPRING_PROFILES_ACTIVE",
        "value": "stu3,test,multitenant"
      }
    ],
    "mountPoints": [
      {
        "readOnly": false,
        "containerPath": "/var/indexes",
        "sourceVolume": "indexes"
      }
    ]
  }
]
